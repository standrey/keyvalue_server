{
  "version": 3,
  "cmakeMinimumRequired":{
    "major": 3,
    "minor": 21,
    "patch": 0
  },
  "configurePresets": [
    { 
      "name": "base-release",
      "hidden": true,
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "EXCEPTIONS": "OFF",
        "CMAKE_BUILD_TYPE": "Debug"
      },
      "warnings": {
        "uninitialized": true,
        "dev": true,
        "deprecated": true
      },
      "architecture": {
        "value": "unknown",
        "strategy": "external"
      }
    }
  ],
  "buildPresets":[
  ]
}
